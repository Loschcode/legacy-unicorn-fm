<div id="join-datas" data-owner="<%= owner %>" data-server="<%= server %>"></div>

<div class="notice notice-black">
    <span id="current-track">No current track</span>
</div>

<div class="spacer50"></div>

<div class="container text-center">

    <div class="col-md-6 col-md-offset-3">
    <div class="page-header">
            <h1>
                <strong>Unicorn.</strong> <br/>
                <small>Manage your music like a boss</small><br/>
                <div class="spacer10"></div>
                <% if (owner == true) { %>
                    <small><span class="label label-danger">Server #<%= server %></span></small>
                <% } else { %>
                    <small><span class="label label-warning">Joined #<%= server %></span></small>
                <% } %>
            </h1>
        </div>

    <form method="post" action="#/search" class="form-inline">
    	<input id="search-track" class="form-control input-lg" type="text" name="search" placeholder="Search track to listen" autocomplete="off" spellcheck="false"/>
    
    	<button type="submit" class="btn btn-black btn-lg" href="#/search">Search</button>
        <a id="clear-results" class="btn btn-danger btn-lg hidden"><i class="fa fa-times"></i></a>

    </form>


    <hr>
</div>
    
    <div class="clear"></div>
    <div class="spacer20"></div>

    <div id="results-search">
    </div>

<div class="container">

    <div id="player">
    </div>

 
</div>

